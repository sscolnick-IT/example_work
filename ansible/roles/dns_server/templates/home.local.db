
$TTL 86400
@ IN SOA dns-01.home.local. admin.home.local. (
    2023062101 ;Serial
    3600 ;Refresh
    1800 ;Retry
    604800 ;Expire
    86400 ;Minimum TTL
)

;Name Server Information
@ IN NS    dns-01.home.local.


;IP for Name Server
dns-01    IN A 192.168.0.97

